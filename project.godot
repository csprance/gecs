; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Zombies Ate My Neighbors!"
config/description="A clone of Zombies Ate my Neighbors in Godot"
config/version="0.0.1"
run/main_scene="res://game/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://game/assets/icons/zamn-logo.png"

[audio]

manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/pool_3D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"

[autoload]

Loggie="*res://addons/loggie/loggie.gd"
Panku="*res://addons/panku_console/console.tscn"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"
Constants="*res://game/lib/autoloads/constants.tscn"
ECS="*res://addons/gecs/ecs.gd"
GameState="*res://game/lib/autoloads/game_state.gd"

[debug]

file_logging/enable_file_logging=true
settings/stdout/verbose_stdout=true

[display]

window/size/resizable=false
window/size/always_on_top=true

[editor_plugins]

enabled=PackedStringArray("res://addons/easy_velocity_vector_display/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/gecs/plugin.cfg", "res://addons/loggie/plugin.cfg", "res://addons/panku_console/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/qt_tools/plugin.cfg", "res://addons/resonate/plugin.cfg", "res://addons/script-tabs/plugin.cfg")

